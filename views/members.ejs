<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Online Tutor Bot - Members</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="css/style.css">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
</head>
<body>
        <nav class="navbar navbar-default">
                <div class="container">
                  <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                      <span class="sr-only">Toggle navigation</span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="/">Online Tutor Bot</a>
                  </div>
              
                  <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                    </ul>
                    <ul class="nav navbar-nav navbar-right" id="reload">
                      <li><a href="/members">Members</a></li>
                      <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%=user.username%> 
                        <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                          <li><a href="#">Action</a></li>
                          <li role="separator" class="divider"></li>
                          <li><a href="/logout"><span class="fa fa-sign-out"></span>Logout</a></li>
                        </ul>
                      </li>
                    </ul>
                  </div>
                </div>
              </nav>
              

<div class="col-md-12">
   <form action="/members" method="post">
       <div class="form-group ad-30" style="margin-top:8px;">
            <div class="input-group search-style">
                <input class="form-control new-search-bar"  name="username" id="" placeholder="Enter Tutor email" type="text">
                <span class="input-group-btn">
                    <button class="btn input-btn" type="" id="search-form">Search</button>
                </span>
            </div>
        </div>
   </form>
</div>
  


<div class="col-md-12">
        <form action="/post" method="post">
            <div class="form-group ad-30" style="margin-top:8px;">
                 <div class="input-group search-style">
                        <textarea class="form-control" name="problem[statement]" id="msg" placeholder="Enter Questions"></textarea>
                        <select name="problem[tag]" required>
                                <% _.forEach(fields,function(field){%>
                                    <% field._id.forEach(function(sub){%>
                                        <option value="<%=sub%>"><%=sub%></option>
                                    <%})%>
                                <%})%>
                        </select>
                     <span class="" style="margin-top:3px; float:right;">
                         <button class="btn input-btn" type="" id="problem-form">Post</button>
                     </span>
                 </div>
             </div>
        </form>
     </div>
       

<div class="col-md-12">
    <input type="hidden" id="name-user" value="<%= user.username%>">
    <!--input type="hidden" name="name-image" value="<%= user.userImage %>"-->
    <div class="">
        <div class="row">
           
            <div class="col-md-9">
                <div>
                    <% _.forEach(chunks,function(val){%>
                        <div class="category-list">
                            <div class="row">
                                <% _.forEach(val,function(val1){%>
                                <form action="/" method="post" id="favorite">
                                        <div class="item col-md-3">
                                            <a style=" text-decoration: none;" href="#">
                                                <div class="thumbnail">
                                                    <img src="https://placehold.it/500x500>"alt="">
                                                    <div class="caption">
                                                            <h4>

                                                            </h4>                                                        
                                                        <div class="row">
                                                            <div class="col-xs-12 col-md-6">
                                                                <h4 style="color:#4aa1f3"><%= val1.username%></h4>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </a>
                                        </div>
                                </form>
                                <%})%>
                            </div>
                        </div>
                    <%})%>
                </div>
            </div>
        </div>
    </div>
</div>

  
    
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/socket.io/socket.io.js">
</script>
<script src="/js/group/sendrequest.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>  
<script src="/js/home.js"></script>
<script src="/js/global.js"></script>
</body>
</html>